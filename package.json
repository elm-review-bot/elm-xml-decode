{
  "name": "elm-xml-decode",
  "version": "1.0.0",
  "description": "Xml Decoder for Elm",
  "devDependencies": {
    "elm": "^0.18.0",
    "elm-analyse": "^0.12.1",
    "elm-test": "^0.18.10",
    "elm-verify-examples": "^1.0.2",
    "watch": "^1.0.2"
  },
  "scripts": {
    "test": "elm-verify-examples && elm-test",
    "watch:test": "watch --interval=1 --ignoreDirectoryPattern='/Doc|elm-stuff/' 'npm test' ./src ./tests",
    "make": "elm-make",
    "package": "elm-package",
    "docs": "elm-make --docs=docs.json",
    "analyse": "elm-analyse",
    "bench:compile": "./scripts/benchmark/compile.sh",
    "bench:install": "./scripts/benchmark/init.sh --install-only",
    "bench:init": "./scripts/benchmark/init.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymtszw/elm-xml-decode.git"
  },
  "keywords": [
    "Xml",
    "Decoder",
    "Elm"
  ],
  "author": "ymtszw",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ymtszw/elm-xml-decode/issues"
  },
  "homepage": "https://github.com/ymtszw/elm-xml-decode#readme"
}
